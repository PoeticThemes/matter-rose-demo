Array.prototype.forEach.call(document.querySelectorAll(".post-card"),function(e){var a=e.querySelector(".post-card-header").querySelector("a");if(a){var t=a.getAttribute("href");t&&(e.querySelector(".post-card-header").addEventListener("click",function(){location.href=t}),e.querySelector(".post-card-excerpt").addEventListener("click",function(){location.href=t}))}}),document.getElementById("background-image")&&imagesLoaded("#background-image",{background:!0},function(){document.getElementById("background-image").style.opacity="1",document.getElementById("background-overlay").style.opacity="1"}),Array.prototype.forEach.call(document.querySelectorAll(".mdl-card__media.has-image"),function(e){imagesLoaded("#background-image",{background:!0},function(){e.getElementsByClassName("card-header-background-image")[0].style.opacity="1",e.getElementsByClassName("card-header-overlay")[0].style.opacity="1"})});